  pod 'Firebase/Auth'
  pod 'Firebase/Firestore'
  pod 'GoogleMaps', '5.1.0'
  pod 'Alamofire'
  pod 'SwiftyJSON'
  
  target 'DBATests' do
    inherit! :search_paths
    pod 'Firebase'
  end

end
